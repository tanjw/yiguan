/* 
 * My97 DatePicker 4.8
 */
body{font-family:"Microsoft Yahei", Arial, Songti, "微软雅黑", "宋体"; font-size:12px;	}
*:focus{outline:0;}

.WdateDiv{
	width:180px;
	background-color:#FFFFFF;
	border:#ccc 1px solid;
	padding:2px;
	border-radius:2px; -webkit-border-radius:2px; -moz-border-radius:2px; -o-border-radius:2px; -khtml-border-radius:2px;
}

.WdateDiv2{
	width:360px;
}
.WdateDiv *{font-size:12px;}

.WdateDiv .NavImg a{
	display:block;
	cursor:pointer;
	height:16px;
	width:16px;
}

.WdateDiv .NavImgll a{
	float:left;
	background:transparent url(img.gif) no-repeat scroll 0 0;
	margin-top:4px;
}
.WdateDiv .NavImgl a{
	float:left;
	background:transparent url(img.gif) no-repeat scroll -16px 0;
	margin-top:4px;
}
.WdateDiv .NavImgr a{
	float:right;
	background:transparent url(img.gif) no-repeat scroll -32px 0;
	margin-top:4px;
}
.WdateDiv .NavImgrr a{
	float:right;
	background:transparent url(img.gif) no-repeat scroll -48px 0;
	margin-top:4px;
}

.WdateDiv #dpTitle{
	height:24px;
	margin-bottom:2px;
	padding:1px;
}

.WdateDiv .yminput{
	margin-top:2px;
	margin-left:4px;	
	text-align:center;
	height:20px;
	border:0px;
	width:50px;
	cursor:pointer;	
	color:#da4f49;
}

.WdateDiv .yminputfocus{
	margin-top:2px;
	margin-left:4px;	
	text-align:center;
	height:20px;
	color:#da4f49;
	border:#ccc 1px solid;
	width:50px;
	border-radius:2px 2px 0 0; -webkit-border-radius:2px 2px 0 0; -moz-border-radius:2px 2px 0 0; -o-border-radius:2px 2px 0 0; -khtml-border-radius:2px 2px 0 0;
}

.WdateDiv .menuSel{
	z-index:1;
	position:absolute;
	background-color:#fff;	
	border:#ccc 1px solid;
	display:none;
}

.WdateDiv .menu{
	cursor:pointer;
	color:#333;
}

.WdateDiv .menuOn{
	cursor:pointer;
	background-color:#f7f7f7;
	color:#333;
}

.WdateDiv .invalidMenu{
	color:#aaa;
}

.WdateDiv .YMenu{
	margin-top:20px;
	padding:2px;
	border-radius:0 2px 2px 2px; -webkit-border-radius:0 2px 2px 2px; -moz-border-radius:0 2px 2px 2px; -o-border-radius:0 2px 2px 2px; -khtml-border-radius:0 2px 2px 2px;
}

.WdateDiv .MMenu{
	margin-top:20px;
	*width:62px;
	border-radius:0 2px 2px 2px; -webkit-border-radius:0 2px 2px 2px; -moz-border-radius:0 2px 2px 2px; -o-border-radius:0 2px 2px 2px; -khtml-border-radius:0 2px 2px 2px;
	padding:2px;
}

.WdateDiv .hhMenu{
	margin-top:-90px; 
	margin-left:26px;
}

.WdateDiv .mmMenu{
	margin-top:-46px; 
	margin-left:26px;
}

.WdateDiv .ssMenu{
	margin-top:-24px; 
	margin-left:26px;
}

 .WdateDiv .Wweek {
 	text-align:center;
	background:#DAF3F5;
	border-right:#BDEBEE 1px solid;
 }

.WdateDiv .WdayTable tr.MTitle,.WdateDiv .WdayTable tr.MTitle:hover{
	background-color:#f7f7f7;
}
.WdateDiv .WdayTable2{
	border-collapse:collapse;
	border:#c5d9e8 1px solid;
}
.WdateDiv .WdayTable2 table{
	border:0;
}

.WdateDiv .WdayTable{
}
.WdateDiv .WdayTable tr{
	background:#fff;
}
.WdateDiv .WdayTable tr:hover{
	background:#fbfbfb;
}
.WdateDiv .WdayTable td{
	text-align:center;
	padding:4px 0;
}

.WdateDiv .Wday{
	cursor:pointer;
}

.WdateDiv .WdayOn{
	cursor:pointer;
	background-color:#e7e7e7;
}

.WdateDiv .Wwday{
	cursor:pointer;
	color:#333;
}

.WdateDiv .WwdayOn{
	cursor:pointer;
	color:#333;
	background-color:#e7e7e7;
}
.WdateDiv .Wtoday{
	cursor:pointer;
	color:#da4f49;
}
.WdateDiv .Wselday{
	background-color:#da4f49;
	color:#fff;
	border-radius:2px; -webkit-border-radius:2px; -moz-border-radius:2px; -o-border-radius:2px; -khtml-border-radius:2px;
}
.WdateDiv .WspecialDay{
	background-color:#66F4DF;
}

.WdateDiv .WotherDay{ 
	cursor:pointer;
	color:#bbb;
}

.WdateDiv .WotherDayOn{ 
	cursor:pointer;
	color:#bbb;
	background-color:#e7e7e7;	
}

.WdateDiv .WinvalidDay{
	color:#aaa;
}

.WdateDiv #dpTime{
	float:left;
	margin-top:3px;
	margin-right:30px;
}

.WdateDiv #dpTime #dpTimeStr{
	margin-left:1px;
}

.WdateDiv #dpTime input{
	width:18px;
	height:20px;
	text-align:center;
	border:#ccc 1px solid;	
}

.WdateDiv #dpTime .tB{
	border-right:0px;
}

.WdateDiv #dpTime .tE{
	border-left:0;
	border-right:0;
}

.WdateDiv #dpTime .tm{
	width:7px;
	border-left:0;
	border-right:0;
}

.WdateDiv #dpTime #dpTimeUp{
	height:10px;
	width:13px;
	border:0px;
	background:url(img.gif) no-repeat -32px -16px;
}

.WdateDiv #dpTime #dpTimeDown{
	height:10px;
	width:13px;
	border:0px;
    background:url(img.gif) no-repeat -48px -16px;
}

 .WdateDiv #dpQS {
 	float:left;
	margin-right:3px;
	margin-top:2px;
	background:url(img.gif) no-repeat 0px -16px;
	width:20px;
	height:20px;
	cursor:pointer;
	display:none;
 }
.WdateDiv #dpControl {
	text-align:center;	
}
.WdateDiv .dpButton{ 
	height:20px;
	width:45px;
	border:0;
	margin-top:2px;
	margin-right:1px;
	background:#fff;
	color:#da4f49;
	cursor:pointer;
}